{
  "operationName": "addCompanies",
  "variables": {
    "companyInputs": [
      {
        "code": "string",                    // 会社コード
        "location": [
          {
            "postalCode": "string",          // 郵便番号
            "prefecture": "string",          // 都道府県
            "address": "string",             // 住所
            "roomLocation": {
              "building": "string",          // 建物名
              "roomNo": "string"             // 部屋番号
            }
          }
        ],
        "telephoneNo": [
          {
            "number": "string",              // 電話番号
            "type": "MOBILE",                // 電話タイプ
            "isPrimary": true                // 主な電話番号かどうか
          }
        ],
        "contactPersonFamilyName": "string", // 担当者の姓
        "mailto:contactpersonmail": "string", // 担当者のメールアドレス
        "mainHrAdmin": "string",             // メイン人事管理者
        "medicalCheckupStartDate": "string", // 健康診断開始日
        "medicalCheckupEndDate": "string",   // 健康診断終了日
        "offices": [],                       // オフィス一覧（空配列）
        "departments": [],                   // 部署一覧（空配列）
        "displayNameMap": {
          "en": "string",                    // 英語会社名
          "ja": "string"                     // 日本語会社名
        },
        "designations": [],                  // 役職一覧（空配列）
        "status": "ACTIVE"                   // ステータス
      }
    ]
  }
}


{
  "operationName": "addOffices",
  "variables": {
    "officeInputs": [
      {
        "code": "string",                    // オフィスコード
        "shortcode": "string",               // オフィス略称コード
        "displayNameMap": {
          "en": "string",                    // 英語オフィス名
          "ja": "string"                     // 日本語オフィス名
        },
        "physicianList": [],                 // 医師一覧（空配列）
        "assignedDepartments": [             // 割り当てられた部署一覧
          "string"                           // 部署ID
        ],
        "status": "ACTIVE"                   // ステータス
      }
    ]
  }
}

{
