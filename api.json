// Company
{
  "operationName": "addCompanies",
  "variables": {
    "companyInputs": [
      {
        "code": "string",                    // 회사 코드
        "location": [
          {
            "postalCode": "string",          // 우편번호
            "prefecture": "string",          // 도도부현
            "address": "string",             // 주소
            "roomLocation": {
              "building": "string",          // 건물명
              "roomNo": "string"             // 방 번호
            }
          }
        ],
        "telephoneNo": [
          {
            "number": "string",              // 전화번호
            "type": "MOBILE",                // 전화 타입
            "isPrimary": true                // 기본 전화번호 여부
          }
        ],
        "contactPersonFamilyName": "string", // 담당자 성
        "mailto:contactpersonmail": "string", // 담당자 이메일
        "mainHrAdmin": "string",             // 메인 HR 관리자
        "medicalCheckupStartDate": "string", // 건강검진 시작일
        "medicalCheckupEndDate": "string",   // 건강검진 종료일
        "offices": [],                       // 사무소 목록 (빈 배열)
        "departments": [],                   // 부서 목록 (빈 배열)
        "displayNameMap": {
          "en": "string",                    // 영어 회사명
          "ja": "string"                     // 일본어 회사명
        },
        "designations": [],                  // 직책 목록 (빈 배열)
        "status": "ACTIVE"                   // 상태
      }
    ]
  }
}

// Office
