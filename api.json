          "en": "string",                    // 영어 사무소명
          "ja": "string"                     // 일본어 사무소명
        },
        "physicianList": [],                 // 의사 목록 (빈 배열)
        "assignedDepartments": [             // 할당된 부서 목록
          "string"                           // 부서 ID
        ],
        "status": "ACTIVE"                   // 상태
      }
    ]
  }
}

